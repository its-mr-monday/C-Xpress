{
    "name": "cxpress",
    "displayName": "cxpress-syntax-highlighter",
    "description": "cxpress language syntax highlihter",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "cxpress",
            "aliases": ["cXpress", "cxpress"],
            "extensions": [".cx"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "cxpress",
            "scopeName": "source.cx",
            "path": "./syntaxes/cxpress.tmLanguage.json"
        }]
    }
}